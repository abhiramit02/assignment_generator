{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api" },
    { "src": "/(.*)", "dest": "/", "headers": { "cache-control": "s-maxage=0" }},
    { "src": "/static/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" }},
    { "src": "/favicon.ico", "headers": { "cache-control": "public, max-age=3600" }},
    { "src": "/manifest.json", "headers": { "cache-control": "public, max-age=3600" }}
  ]
}